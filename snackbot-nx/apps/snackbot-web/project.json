{
  "name": "snackbot-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/snackbot-web/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/snackbot-web",
        "command": "npm run build"
      },
      "configurations": {
        "production": {}
      },
      "outputs": ["{workspaceRoot}/apps/snackbot-api/static/site"]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/snackbot-web",
        "command": "npm run dev"
      }
    }
  }
}
